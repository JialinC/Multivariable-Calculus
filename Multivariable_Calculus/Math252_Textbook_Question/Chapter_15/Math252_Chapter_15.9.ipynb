    "import sympy as sp\n",
    "x,y,z,rho,theta,phi = sp.symbols(\"x y z rho theta phi\",real=True)\n",
    "V=sp.integrate(sp.integrate(sp.integrate(rho**2*sp.sin(phi),(rho,0,sp.cos(phi))),(phi,0,sp.pi/4)),(theta,0,2*sp.pi))\n",
    "print(V)"
